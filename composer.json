{
    "name": "cexll/hyperf-apihash",
    "description": "API Response Body Hash In (RSA AND AES)",
    "license": "MIT",
    "type": "project",
    "keywords": [
        "php",
        "hyperf",
        "apihash"
    ],
    "require": {
        "php": ">=7.2",
        "ext-openssl": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "Cexll\\Hyperf\\Apihash\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1"
    }
}
